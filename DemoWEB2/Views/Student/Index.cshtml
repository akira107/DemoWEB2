@model IEnumerable<DemoWEB2.Models.StudentInfo>
@{
    ViewData["Title"] = "Ket qua sinh vien";
}
<h1>Ket qua sinh vien</h1>
<table class="table">
    <tr>
        <td>MSSV</td>
        <td>Ten</td>
        <td>Diem</td>
    </tr>


@foreach(var i in Model)
{
    <tr>
        <td>@i.Id</td>
        <td>@i.Name</td>
        <td>@i.Marks</td>
    </tr>
}
</table>

<label>Tong so sv: </label> @ViewBag.TotalStudents